           def show_pandaser(response):
        if response.status==200 or response.status==0:
            panda = json.loads(response.text)
            document["deef"].html = {{panda}}
            #document["deef"].alert("bleh")
        else:
            warnings.warn(response.text)

    def get_panadaser(ev):
        req = ajax.ajax()
        req.bind('complete', show_pandaser)
        req.open('POST', '/pandaser', True)
        req.set_header('content-type','application/json')
        data = json.dumps({'comic_input': document['comic_input'].value})
        req.send(data) 
        




    def show_pandaser(response):
        if response.status==200 or response.status==0:
            panda = json.loads(response.text)
            document["deef"].html = panda
            #document["deef"].alert("bleh")
        else:
            warnings.warn(response.text)

    def get_panadaser(ev):
      req = ajax.ajax()
      req.bind('complete', show_pandaser)
      req.open('POST', '/pandaser', True)
      req.set_header('content-type','application/json')
      data = json.dumps({'comic_input': document['comic_input'].value})
      req.send(data) 
      
    document["df_button"].bind("click", get_panadaser) 


